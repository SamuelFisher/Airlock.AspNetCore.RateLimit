<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Airlock.AspNetCore.RateLimit</PackageId>
    <TargetFramework>netstandard2.0</TargetFramework>    
    <Description>Rate limiting for ASP.NET Core MVC.</Description>
    <Authors>Samuel Fisher</Authors>
    <VersionPrefix>0.0.1</VersionPrefix>
    <PackageProjectUrl>https://github.com/SamuelFisher/Airlock.AspNetCore.RateLimit</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/SamuelFisher/Airlock.AspNetCore.RateLimit/master/LICENSE</PackageLicenseUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.1.3" />
  </ItemGroup>

</Project>
